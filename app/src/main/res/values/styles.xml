<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="GWProgressViewStyle">
        <item name="progress_stroke_color">?attr/colorAccent</item>
        <item name="progress_stroke_width">2dp</item>
        <item name="progress_radius">20dp</item>
        <item name="progress_immediate">true</item>
        <item name="progress_duration">3000</item>
        <item name="progress_interpolator">@android:interpolator/accelerate_decelerate</item>
        <item name="progress_background_color">#88000000</item>
    </style>
</resources>
